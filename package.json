{
  "name": "@midgar/route",
  "version": "1.0.0-alpha.1.0",
  "description": "A Migdar plugin to load route",
  "main": "src/index.plugin.js",
  "module": "true",
  "scripts": {
    "build-docs": "jsdoc -c ./jsdoc.js ./README.md -t ./node_modules/ink-docstrap/template ./README.md -r ./src/",
    "test": "mocha --require esm test/*.js --exit",
    "coverage": "nyc -i esm --reporter=lcov --reporter=text-summary npm run test",
    "eslint:ci": "eslint -f json -o reports/.eslint-report.json src/ || exit 0",
    "test:ci": "mocha --require esm test/*.js --reportermocha-sonarqube-reporter --reporter-options output=reports/xunit.xml --exit",
    "coverage:ci": "nyc -i esm --reporter=lcov npm run test:ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/midgarjs/controller.git"
  },
  "dependencies": {
    "@midgar/express": "git+https://gitea.midgar.io/Midgar/express.git",
    "@midgar/midgar": "git+https://gitea.midgar.io/Midgar/midgar.git",
    "@midgar/service": "git+https://gitea.midgar.io/Midgar/service.git",
    "@midgar/utils": "^1.0.0-alpha.1.5",
    "express-validator": "^6.4.0",
    "htmlencode": "^0.0.4",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "esm": "^3.2.25",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.4",
    "mocha": "^7.1.1",
    "mocha-sonarqube-reporter": "^1.0.1",
    "nyc": "^15.0.1"
  },
  "keywords": [
    "midgar",
    "mvc",
    "mvcs",
    "express",
    "router",
    "validator",
    "controller"
  ],
  "author": "Metais Fabien",
  "license": "MIT"
}
